    key <AE08> { [ d, D, eth, ETH, U2212, onequarter, F9, F9 ] }; // d D ð Ð − ¼ 8 8

    //key <AE09> { type[group1] = "FOUR_LEVEL_SEMIALPHABETIC", [           slash,            9,       division,        onehalf ] }; // / 9 ÷ ½
    //key <AE09> { type[group1] = "FOUR_LEVEL_SEMIALPHABETIC", [           slash,            9,       division,        onehalf ] }; // / 9 ÷ ½
    key <AE09> { [ l, L, dead_stroke, NoSymbol, division, onehalf, F10, F10 ] }; // l L /  ÷  9 9

    //key <AE10> { type[group1] = "FOUR_LEVEL_SEMIALPHABETIC", [        asterisk,            0,       multiply,  threequarters ] }; // * 0 × ¾
    key <AE10> { [ j, J, U0133, U0132, multiply, threequarters, F11, F11 ] }; // j J ĳ Ĳ × ¾ 0 0

    //key <AE11> { [           equal,       degree,       notequal,        minutes ] }; // = ° ≠ ′
    key <AE11> { [ z, Z, schwa, SCHWA, notequal, NoSymbol, F12, F12 ] }; // z Z ə Ə

    //key <AE12> { [         percent,        grave,       permille,        seconds ] }; // % ` ‰ ″
    key <AE12> { [ NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] }; // % ` ‰ ″

    // Second row
    //key <AD01> { [               b,            B,            bar,      brokenbar ] }; // b B | ¦
    key <AD01> { [ a, A, ae, AE, quotedbl, NoSymbol, 1, 1  ] }; // a A æ Æ "  1 1

    //key <AD02> { [          eacute,       Eacute,     dead_acute, dead_doubleacute ] }; // é É ˊ ˝
    key <AD02> { [ u, U, ugrave, Ugrave, guillemotleft, NoSymbol, 2, 2 ] }; // u U ù Ù « 2 2

    //key <AD03> { [               p,            P,      ampersand,        section ] }; // p P & §
    key <AD03> { [ i, I, dead_diaeresis,  dead_abovedot, guillemotright, NoSymbol, 3, 3 ] }; // i I ̈ ˙ » 3 3

