    key <AD10> { [ n, N, dead_tilde, asterisk, NoSymbol, 0, 0 ] }; // n N ~ * 0 0

    //key <AD11> { [               z,            Z,          schwa,          SCHWA ] }; // z Z ə Ə
    key <AD11> { [ m, M, dead_macron, masculine, equal, NoSymbol, BackSpace, Backspace ] }; // m M ̄ º = BS BS

    // no w key on planck
    key <AD12> { [ NoSymbol, NoSymbol, NoSymbol, NoSymbol,  NoSymbol,  NoSymbol,  NoSymbol,  NoSymbol ] };

    // Third row
