    key <AE05> { [ egrave, Egrave, dead_grave, grave,  bracketright, NoSymbol, F6, F6 ] }; // è È ` ` ] ]

    //key <AE06> { type[group1] = "FOUR_LEVEL_SEMIALPHABETIC", [              at,            6,    asciicircum                 ] }; // @ 6 ^
