    key <AD04> { [ e, E, EuroSign, dead_currency, parenleft, NoSymbol, 4, 4 ] }; // e E € ( 4 4

    //key <AD05> { [          egrave,       Egrave,     dead_grave,          grave ] }; // è È ` `
    key <AD05> { [ comma, semicolon, rightsinglequotemark, dead_horn, parenright, NoSymbol, 5, 5 ] }; // , ; ’ ̛) 5 5

    //key <AD06> { [ dead_circumflex,       exclam,     exclamdown                 ] }; // ^ ! ¡
    key <AD06> { [ c, C, copyright, U017F, at, NoSymbol, 6, 6 ] }; // c C © ſ @  6 6

    //key <AD07> { [               v,            V,     dead_caron                 ] }; // v V ˇ
    key <AD07> { [ t, T, thorn, THORN, plus, NoSymbol, 7, 7 ] }; // t T þ Þ +  7 7

    //key <AD08> { [               d,            D,            eth,            ETH ] }; // d D ð Ð
    key <AD08> { [ s, S, ssharp, U1E9E, minus, NoSymbol, 8, 8 ] }; // s S ß ẞ - 8 8

    //key <AD09> { [               l,            L,    dead_stroke                 ] }; // l L /
    key <AD09> { [ r, R, registered, trademark, slash, NoSymbol, 9, 9 ] }; // r R ® ™ / 9 9

